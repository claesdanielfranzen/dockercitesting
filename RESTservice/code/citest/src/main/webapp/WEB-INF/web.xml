<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>CItest</display-name>
  <servlet>
  	<servlet-name>CITestServlet</servlet-name>
  	<display-name>CITestServlet</display-name>
  	<description>Beskrivning av CITestServlet</description>
  	<servlet-class>citestpackage.CITest</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CITestServlet</servlet-name>
  	<url-pattern>/CITestServlet</url-pattern>
  </servlet-mapping>
  
	<resource-env-ref>
	    <resource-env-ref-name>jdbc/db</resource-env-ref-name>
	    <resource-env-ref-type>javax.sql.DataSource</resource-env-ref-type>
	</resource-env-ref>  
  
</web-app>
